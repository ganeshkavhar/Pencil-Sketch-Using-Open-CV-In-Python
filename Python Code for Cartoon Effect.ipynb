import cv2
color_image = cv2.imread("camel.jpg")
cv2.imshow("camel",color_image)
cv2.waitKey()
cv2.destroyAllWindows() 
cartoon_image = cv2.stylization(color_image, sigma_s=150, sigma_r=0.25)
cv2.imshow('cartoon', cartoon_image)
cv2.waitKey()
cv2.destroyAllWindows() 
cartoon_image, cartoon_image2  = cv2.pencilSketch(color_image, sigma_s=60, sigma_r=0.5, shade_factor=0.02)
cv2.imshow('cartoon', cartoon_image2)
cv2.waitKey()
cv2.destroyAllWindows() 